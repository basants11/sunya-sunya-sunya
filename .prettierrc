{
  "$schema": "https://json.schemastore.org/prettierrc",
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "singleAttributePerLine": false,
  "htmlWhitespaceSensitivity": "css",
  "vueIndentScriptAndStyle": false,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "insertPragma": false,
  "requirePragma": false,
  "水利": false,
  "embeddedLanguageFormatting": "auto",
  "overrides": [
    {
      "files": ["*.md", "*.mdx"],
      "options": {
        "printWidth": 80,
        "tabWidth": 2,
        "useTabs": false,
        "semi": false,
        "singleQuote": false,
        "bracketSpacing": false,
        "proseWrap": "always"
      }
    },
    {
      "files": ["*.json", "*.jsonc"],
      "options": {
        "printWidth": 80,
        "tabWidth": 2,
        "useTabs": false,
        "semi": true,
        "singleQuote": false,
        "trailingComma": "none"
      }
    },
    {
      "files": ["*.yaml", "*.yml"],
      "options": {
        "printWidth": 80,
        "tabWidth": 2,
        "useTabs": false,
        "semi": false,
        "singleQuote": false,
        "trailingComma": "none"
      }
    },
    {
      "files": ["*.css", "*.scss", "*.sass"],
      "options": {
        "printWidth": 120,
        "tabWidth": 2,
        "useTabs": false,
        "singleQuote": true
      }
    },
    {
      "files": ["*.html", "*.htm"],
      "options": {
        "printWidth": 120,
        "tabWidth": 2,
        "useTabs": false,
        "singleQuote": true
      }
    },
    {
      "files": ["*.graphql", "*.gql"],
      "options": {
        "printWidth": 120,
        "tabWidth": 2,
        "useTabs": false,
        "singleQuote": true,
        "trailingComma": "none"
      }
    },
    {
      "files": ["*.sql"],
      "options": {
        "printWidth": 120,
        "tabWidth": 2,
        "useTabs": false,
        "keywordCase": "upper"
      }
    },
    {
      "files": ["*.sh", "*.bash"],
      "options": {
        "printWidth": 120,
        "tabWidth": 2,
        "useTabs": false,
        "semi": false
      }
    }
  ]
}
